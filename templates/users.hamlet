  <tr>
    <th> User ID
    <th> Name
    <th> Dues Rate
    <th> Actions
  $forall (E.Value usrId, E.Value usrident, E.Value usrName, E.Value drname, E.Value dramt) <- users
    <tr> 
      <td> <a href=@{UserR usrId}> #{usrident} 
      <td> #{usrName} 
